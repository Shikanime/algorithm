{
  "version": 3,
  "configurePresets": [
    {
      "name": "unknown-nixos-gnu",
      "displayName": "NixOS",
      "description": "Sets Ninja generator, build and install directory",
      "generator": "Ninja",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "CMAKE_MAKE_PROGRAM": "ninja",
        "CMAKE_CXX_COMPILER": "g++",
        "CMAKE_C_COMPILER": "gcc"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "unknown-nixos-gnu",
      "configurePreset": "unknown-nixos-gnu"
    }
  ],
  "testPresets": [
    {
      "name": "unknown-nixos-gnu",
      "configurePreset": "unknown-nixos-gnu",
      "output": {
        "outputOnFailure": true
      },
      "execution": {
        "stopOnFailure": true
      }
    }
  ]
}